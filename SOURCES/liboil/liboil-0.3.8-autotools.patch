! 	;;
!       esac
!     fi
!   done
!   IFS="$lt_save_ifs"
! else
!   lt_cv_path_LD="$LD" # Let the user override the test with a path.
! fi])
! LD="$lt_cv_path_LD"
! if test -n "$LD"; then
!   AC_MSG_RESULT($LD)
! else
!   AC_MSG_RESULT(no)
! fi
! test -z "$LD" && AC_MSG_ERROR([no acceptable ld found in \$PATH])
! AC_PROG_LD_GNU
! ])# AC_PROG_LD
