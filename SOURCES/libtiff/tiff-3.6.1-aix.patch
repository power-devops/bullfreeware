+         DSOSUF_VERSION=${DSOSUF}.${DIST_MINOR}.${DIST_POINT}
+         LIBCOPTS='-fPIC'
+         DSOOPTS='-shared'
+         DSO=OPENSERVER
+         ;;
+ 
+     esac
+ fi
+ if [ "$DSO" != no ]; then
+     JUNK="$JUNK t.${DSOSUF}"
+     #
+     # Check to make sure the compilers process
+     # the DSO options in the expected way.
+     #
+     CheckCCDSO()
+     {
+     $RM t.c t.o t.${DSOSUF}
+     cat>t.c<<EOF
+ int f() { return 0; }
+ EOF
+     capture cat t.c
+     runMake t \
+ 	"t.o:; ${CCOMPILER} ${ENVOPTS} ${LIBCOPTS} -c t.c" \
+ 	"t: t.o; ${DSOLD} ${DSOOPTS} -o t.${DSOSUF} t.o"
