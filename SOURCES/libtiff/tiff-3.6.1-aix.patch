+ fi
+ test -z "$TIFFLIBREF" && TIFFLIBREF="\${DEPTH}/libtiff/libtiff.${DSOSUF}"
+ test -z "$DSOSUF_VERSION" && DSOSUF_VERSION=${DSOSUF}
