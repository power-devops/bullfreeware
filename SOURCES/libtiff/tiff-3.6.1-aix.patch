+ 		@LIBJPEG@ @LIBGZ@ @MACHDEPLIBS@
+ 	rm -f libtiff.@DSOSUF@
+ 	if [ @DSOSUF_VERSION@ != @DSOSUF@ ] ; then \
+ 	  @LN@ @LN_S@ libtiff.@DSOSUF_VERSION@ libtiff.@DSOSUF@; \
+ 	fi
