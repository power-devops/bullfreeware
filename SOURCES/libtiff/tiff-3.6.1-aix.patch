+ AIXdso: ${OBJS}
+ 	rm -f libtiff.syms shr.o
+ 	echo "#!" > libtiff.syms
+ 	/bin/dump -g libtiff.a | sed -n -e \
+ 	      's/^[ 	]*[0-9][0-9]*[	 ]*\([^	 .][^ 	]*\)$$/\1/p' \
+ 	      >> libtiff.syms
+ 	${LD} -o shr.o libtiff.a -H512 -T512 -bM\:SRE \
+ 	    -bE\:libtiff.syms @LIBJPEG@ @LIBGZ@ -lc -lm -L@DIR_LIB@
+ 	rm -f libtiff.syms libtiff.@DSOSUF@
+ 	${AR} ${AROPTS} libtiff.@DSOSUF@ shr.o
+ 	rm -f shr.o
+ 	touch $@
