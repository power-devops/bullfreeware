+ 	[yY]*|[nN]*)	continue;;
+ 	?*)
+ 	    echo ""
+ 	    echo "\"y\", \"yes\", or <RETURN> accepts the displayed parameters."
+ 	    echo "A number lets you change the numbered parameter."
+ 	    echo ""
+ 	    ;;
+ 	esac
+ 	ok=skip
+     done
